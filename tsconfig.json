{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "DOM",
      "ES2020"
    ],
    "module": "ESNext",
    "jsx": "react-jsx",
    "types": [
      "node",
      "react",
      "react-dom"
    ],
    "noEmit": true,
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "allowImportingTsExtensions": true,
    "baseUrl": ".",
    "paths": {
      "@theme": [ "node_modules/rspress/theme" ],
      "@components/*": [ "src/components/*"],
    },
  },
  "include": [
    "docs",
    "theme",
    "rspress.config.ts"
  ],
  "mdx": {
    "checkMdx": true
  }
}